{
  "name": "powerbelt",
  "version": "0.0.0",
  "description": "JavaScript Utility Belt",
  "keywords": [
    "utility"
  ],
  "homepage": "https://github.com/jesselpalmer/powerbelt#readme",
  "bugs": {
    "url": "https://github.com/jesselpalmer/powerbelt/issues"
  },
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test mocha './src/**/*.spec.js' --compilers js:babel-register",
    "test:unit:watch": "NODE_ENV=test mocha './src/**/*.spec.js' -w --compilers js:babel-register",
    "cover": "istanbul cover _mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": "git+https://github.com/jesselpalmer/powerbelt.git",
  "author": "Jesse Palmer <jesselpalmer@gmail.com> (http://jesselpalmer.com)",
  "contributors": [
    {
      "name": "Jesse Palmer",
      "email": "jesselpalmer@gmail.com",
      "url": "jesselpalmer.com"
    }
  ],
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-preset-env": "1.1.8",
    "babel-preset-es2015": "6.22.0",
    "babel-register": "6.22.0",
    "chai": "3.5.0",
    "coveralls": "2.11.15",
    "eslint": "3.14.1",
    "eslint-config-airbnb-base": "11.0.1",
    "eslint-plugin-import": "2.2.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "mocha-junit-reporter": "1.13.0",
    "mocha-lcov-reporter": "1.2.0"
  },
  "xo": {
    "envs": [
      "jasmine"
    ]
  },
  "engines": {
    "node": ">=7.4.0"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
